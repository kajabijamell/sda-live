<div class="nested-fields col-3">
  <div class="form-group">
    <%= f.label :platform %>
    <%= f.text_field :platform, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :url %>
    <%= f.text_field :url, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :stream_key %>
    <%= f.text_field :stream_key, class: 'form-control' %>
  </div>
  <%= link_to_remove_association "Remove", f, class: 'btn btn-danger' %>
</div>